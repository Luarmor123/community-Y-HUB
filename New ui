RunService.RenderStepped:Connect(function(frame)
            local Current_Ping = string.split(game:GetService("Stats").Network.ServerStatsItem["Data Ping"]:GetValueString(), " ")[1] .. "ms"
            local fps = math.round(1/frame)
if fps < 15 then
LabelInfo.Text = tostring(Current_Ping) .. " | ⚠️ FPS: " .. tostring(fps) .. " ⚠️"
Fps_Notify = true
else
LabelInfo.Text = tostring(Current_Ping) .. " | FPS: " .. tostring(fps) .. ""
end
            end)
        end

