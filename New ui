            LabelInfo.Text = "--/--"
            LabelInfo.TextColor3 = Color3.fromRGB(255, 255, 255)
            LabelInfo.TextSize = 9.000
            LabelInfo.TextTransparency = 0.300
            for _, v in ipairs(game:GetService("HttpService"):JSONDecode(game:HttpGetAsync("https://games.roblox.com/v1/games/" .. game.PlaceId .. "/servers/Public?sortOrder=Asc&limit=100")).data) do
            local GetRandomNumberPlayer = tostring(v.maxPlayers) .. "/" .. tostring(v.playing)
            
            local Players = game.Players
Players.PlayerAdded:Connect(function(player)
            LabelInfo.Text = GetRandomNumberPlayer
            end)
            
            local Players = game.Players
Players.PlayerRemoving:Connect(function(player)
            LabelInfo.Text = GetRandomNumberPlayer
            end)
            end
        end

