  Name = "Transmission time ( Sec )",
  MinValue = spamInterval,
  MaxValue = 100,
  Default = spamInterval,
  Increase = 1,
  Callback = function(val)
  spamInterval = val
    
  end
})
local Toggle = AddToggle(Chat, {
  Name = "Spam",
  Default = false,
  Callback = function(val)if spamMessage == "" then
        else
            spaming = val

            if spaming then
                repeat wait(spamInterval)
                    chat(spamMessage)
                until spaming == false
            end
        end
        
    end
})









