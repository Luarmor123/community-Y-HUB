    Name = "Ø£Ø®ØªÙŠØ§Ø± Ù„Ø§Ø¹Ø¨",
    Default = "",
    Options = getPlayerList(),
    Callback = function(value)
        selectedPlayer = value
        PlayerTP = value
    end
})
