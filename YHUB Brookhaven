    Name = "Ø§Ù„ÙˆØ§Ù† ØªÙ„Ù‚Ø§Ø¦ÙŠØ© ( Ø§Ù„Ø¬Ø³Ù… )",
    Default = false,
    Callback = function(Value)
        AutoColorToggle(Value)
    end
})


