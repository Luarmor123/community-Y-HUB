local soundId = "rbxassetid://15689451063"

-- الكود البرمجي
local function playSound()
    local sound = Instance.new("Sound")
    sound.SoundId = soundId
    sound.Parent = game.Workspace -- ضعه في مساحة العمل للتأكد من سماعك
    sound:Play()
end

-- اياك ان تحذفه !
playSound()


