local colors = {
    "Earth green",
    "Magenta",
    "Maroon",
    "Toothpaste",
    "Institutional white"
}

-- VariÃƒÂ¡vel para controlar o estado do toggle
local isAutoColorEnabled = false

-- FunÃƒÂ§ÃƒÂ£o para alternar automaticamente as cores
local function AutoColorToggle(toggleValue)
    isAutoColorEnabled = toggleValue  -- Atualiza o estado do toggle

    -- Se o toggle for desativado, nÃƒÂ£o faz mais nada
    if not toggleValue then
        return
    end

    -- Loop para alternar entre as cores
    local index = 1
    while isAutoColorEnabled do
        local args = {
            [1] = "skintone",
            [2] = colors[index]
        }
        game:GetService("ReplicatedStorage").RE:FindFirstChild("1Updat1eAvata1r"):FireServer(unpack(args))
        index = index % #colors + 1  -- AvanÃƒÂ§a para a prÃƒÂ³xima cor circularmente
        wait(2)  -- Espera 3 segundos antes de trocar para a prÃƒÂ³xima cor (ajuste conforme necessÃƒÂ¡rio)
    end
end

-- Exemplo de uso:
