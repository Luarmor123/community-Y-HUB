AddButton(Chat, {
  Name = "Ø£Ø±Ø³Ø§Ù„",
  Callback = function()
         if bypassText ~= "" then
            local bypassedText = bypassMessage(bypassText)

            sendMessage(bypassedText)
        else
            MakeNotifi({
                    Title = "Ø®Ø·Ø£" ,
                    Text = "Ø§Ù„Ø­Ù‚Ù„ ÙØ§Ø±Øº !" ,
                    Time = 6
                })
                end
  end
})

